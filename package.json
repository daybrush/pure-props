    "purecomponent",
    "pure-props"
  ],
  "author": "Daybrush",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/daybrush/react-pure-props/issues"
  },
  "homepage": "https://github.com/daybrush/react-pure-props#readme",
  "dependencies": {
    "@types/react": "^16.8.19",
    "@types/shallowequal": "^1.1.1",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@daybrush/builder": "^0.1.1",
    "print-sizes": "0.0.4",
    "react": "^16.8.6",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1"
  }
}
